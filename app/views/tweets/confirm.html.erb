<div class="col-md-10 col-sm-10 col-sm-offset-1">
<%= form_for(@tweet) do |f| %>
    <%= f.hidden_field :content %>
    <p>内容:<%= @tweet.content %></p>
    <%= f.submit '投稿', class:'btn btn-primary btn-block' %>
<% end %>
<br>
<%= form_for(@tweet, url: new_tweet_path, method: 'get') do |f| %>
    <%= f.hidden_field :content %>
    <%= f.submit '戻る', name: 'back', class:'btn btn-default btn-block'%>
<% end %>
</div>